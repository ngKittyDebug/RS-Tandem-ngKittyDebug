generator client {
  provider      = "prisma-client"
  output        = "../src/generated/prisma"
  moduleFormat  = "cjs"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    String     @default(uuid()) @id
  
  email String  @unique
  password  String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  @@map("users")
}

